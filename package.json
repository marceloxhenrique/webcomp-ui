{
  "name": "webcomp-ui",
  "version": "0.0.1",
  "description": "Web Components library UI.",
  "main": "dist/cjs/lib.js",
  "module": "dist/esm/lib.js",
  "types": "dist/types/lib.d.ts",
  "type": "commonjs",
  "browser": "dist/esm/lib.js",
  "exports": {
    "import": "./dist/esm/lib.js",
    "require": "./dist/cjs/lib.js",
    "types": "./dist/types/lib.d.ts"
  },
  "repository": {
    "url": "https://github.com/marceloxhenrique/universal-ui"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build:esm": "tsc --module ES6 --outDir dist/esm",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build": "npm run build:esm && npm run build:cjs",
    "dev": "tsc --watch"
  },
  "keywords": [
    "Web Components",
    "UI"
  ],
  "author": "marceloxhenrique",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
